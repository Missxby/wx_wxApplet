.item{
  background: white;
  padding: 24rpx 30rpx;
  margin-bottom: 20rpx;
}
.title{
  font-size: 32rpx;
  color: #333333;
}
.des{
  margin-top: 10rpx;
  font-size: 26rpx;
  color: #666666;
}
.time{
  margin-top: 10rpx;
  font-size: 26rpx;
  color: #999999;
}
.box{
  margin-top: 20rpx;
}
.box image{
  display: block;
  width: 100%;
  height: 340rpx;
}
.btns{
  margin-top: 30rpx;
  overflow: hidden;
}
.btns .left{
  float: left;
  width: 128rpx;
  height: 60rpx;
  text-align: center;
  line-height:60rpx; 
  font-size: 26rpx;
  color: #DB5F96;
  border: 1rpx solid #DB5F96;
  border-radius: 4rpx;
}
.btns .right{
  float: right;
  margin-right: 40rpx;
  width: 38rpx;
  height: 36rpx;
}
button{
  position: relative;
  top: 10rpx;
  border:none;
  padding: 0;
  padding-left:0;
  padding-right:0;
  box-sizing:border-box;
  background: white;
  overflow: auto;
  border-radius: 0;
}
button::after{
  border:none;
}
.btns .right button{
  display: block;
  width: 100%;
  height: 100%;
  overflow: auto;
  background: white;
  padding: 0;

}
.btns .right image{
  position: relative;
  display: block;
  width: 100%;
  height: 100%;
}